#!/usr/bin/perl
# USE UTF-8 !!!
package Tomahawk::module;
use open ':utf8', ':std';
use encoding 'utf8';
use utf8;
use strict;

sub execute
{
	my %env=@_;
	# load .xsgn file as defined from TypeID
	Tomahawk::GetXSGN(-convertvars=>1) || return undef;
	
	foreach my $lng(@TOM::LNG_accept)
	{
		my $null=$XSGN{'lng'};
		$null=~s|<%lng%>|$lng|g;
		$XSGN{'TMP'}=~s|<#lng#>|$null|;
	}
	
	return 1;
}

1;