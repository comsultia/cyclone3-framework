     }
     next};
     
    # clanky m0
    $var=/^a400$/ && do
    {

     next};
     
  }}}

  
  

 }



  # NAME OF CATEGORY
 if ($env{show_catname})
 {
  main::_log("show category $env{IDcategory}");
  my $db0=$main::DBH->Query("
	SELECT name FROM $env{db_400}.a400_category
	WHERE	ID='$env{IDcategory}'
		AND (lng='$env{lng}' OR lng='')
		AND active='Y'
	LIMIT 1");
  if (my @db0_line=$db0->fetchrow)
