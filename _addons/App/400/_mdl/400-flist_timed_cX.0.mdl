    main::_log("xrelated app \"$var\" with \"$null\"");
    
    # forum m0
    $var=~/^a820$/ && do
    {
     $env{db_820}=Tomahawk::Getmdlvar("820","db") unless $env{db_820};
     $env{db_820}=$TOM::DB_name unless $env{db_820};
     my $db1=$main::DBH->Query("
	SELECT messages FROM $env{db_820}.a820
	WHERE	ID='$null'
