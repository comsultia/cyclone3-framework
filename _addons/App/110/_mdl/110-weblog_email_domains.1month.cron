sub execute
{
	alarm(3600);
	my %env=@_;

	if ($cron::P eq $CRON::P){$cron::ERR="WARN: this cron is only for local use!!!";return undef}
	
	$env{email} = $TOM::contact{'stats'}.";".$TOM::contact{'TOM'};
	#$env{email} = 'gregor@webcom.sk';
	$env{email} = TOM::Utils::vars::unique_split( $env{email} );
